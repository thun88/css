{
  "non-interactive": false,
  "dry-run": false,
  "requireCleanWorkingDir": true,
  "verbose": false,
  "pkgFiles": ["package.json"],
  "increment": "patch",
  "buildCommand": "npm run build",
  "src": {
    "commitMessage": "Release v%s",
    "tagAnnotation": "Release v%s",
    "beforeStartCommand": "npm i && npm run test"
  },
  "npm": {
    "tag": "latest"
  },
  "github": {
    "release": true,
    "releaseName": "v%s",
    "tokenRef": "GITHUB_ACCESS_TOKEN"
  },
  "prompt": {
    "src": {
      "status": true,
      "commit": false,
      "tag": false,
      "push": false,
      "release": false,
      "publish": false
    }
  }
}
